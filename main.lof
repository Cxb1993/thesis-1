\contentsline {figure}{\numberline {\relax 5.11}{\ignorespaces Scaling of the maximum error norm as a function of Weber ($La = 12000$, $\rho _l / \rho _g = 1000$). \relax }}{48}{figure.caption.71}
\defcounter {refsection}{0}\relax
\contentsline {figure}{\numberline {\relax 5.12}{\ignorespaces Scaling of the maximum error norm as a function of Laplace ($We = 0.4$, $\rho _l / \rho _g = 1000$). \relax }}{48}{figure.caption.72}
